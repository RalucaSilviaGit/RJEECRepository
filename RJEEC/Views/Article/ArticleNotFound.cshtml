@model int

@{
    ViewBag.PageTitle = "RJEEC - 404 Error";
}

<div class="alert alert-danger mt-1 mb-1">
    <h4>404 Not Found Error :</h4>
    <hr />
    <h5>
        Article with ID = @Model cannot be found
    </h5>
</div>

<a asp-controller="article" asp-action="index" class="btn btn-outline-success"
   style="width:auto">Click here to see the list of all articles.</a>